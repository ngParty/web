  "devDependencies": {
    "@types/node": "12.12.7",
    "@types/react": "16.9.11",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "prettier": "1.19.1",
    "typescript": "3.7.2"
  }
